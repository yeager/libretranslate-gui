#!/usr/bin/env python3
import sys
import os
sys.path.insert(0, '/usr/lib/python3/dist-packages')

try:
    from libretranslate_gui.main import main
    main()
except ImportError:
    try:
        from libretranslate_gui.app import main
        main()
    except ImportError:
        print("Error: Cannot find main module for libretranslate-gui", file=sys.stderr)
        sys.exit(1)
